{
  "name": "Proctor Vue",
  "description": "A browser extension for Proctor Vue",
  "version": "0.3.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["dist/background.js"],
    "persistent": false
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  },
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "options_page": "options.html"
}
